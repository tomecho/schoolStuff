Cell[CellGroupData[{
Cell["\<\
MATH355 - Calculus III
Lab #5 Centroid of Triangle with Varying Density\
\>", "Subtitle",
 CellChangeTimes->{{3.419850187984375*^9, 3.419850236484375*^9}, {
  3.46866357877017*^9, 3.468663628468114*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "In this lab we will have ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " do the mathematics of finding the centroid of a triangle region with \
varying density.\nInitially, however, we will do the centroid of the triangle \
with constant density."
}], "Subsection",
 CellChangeTimes->{{3.468663644567761*^9, 3.468663698765976*^9}, {
   3.468663883848887*^9, 3.468663930837829*^9}, 3.6049950565371237`*^9}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"x", ",", "y", ",", "f", ",", "m", ",", "\[Rho]"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{
   RowBox[{"-", "x"}], "+", "6"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"m1", "[", "x_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"-", 
